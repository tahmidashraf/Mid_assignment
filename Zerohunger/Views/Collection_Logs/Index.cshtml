        <td>
            @Html.DisplayFor(modelItem => item.Collection_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Distribution_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Distribution_status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Collect_Requests.Collection_status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Log_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Log_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Log_id })
